generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./test.db"
}

model SurveyResponse {
  id            Int      @id @default(autoincrement())
  firstName     String
  lastName      String
  email         String
  contactNumber String
  dateOfBirth   DateTime
  foods         String
  ratingMovies  Int
  ratingRadio   Int
  ratingEatOut  Int
  ratingTV      Int
  submittedAt   DateTime @default(now())
}
